  },
  "scripts": {
    "start": "concurrently \"npm run start:redis && npm run start:backend\" \"npm run start:frontend\"",
    "start:all": "trap 'npm run stop' SIGINT SIGTERM; concurrently --kill-others-on-fail \"npm run start:redis\" \"redis-cli ping\" \"npm run start:backend\" \"npm run start:frontend\" \"gh codespace ports visibility 5000:public --codespace=$(gh codespace list --json name --jq '.[0].name') || true\" ",
    "dev:all": "concurrently \"npm run dev --prefix backend\" \"npm run dev --prefix frontend\"",
    "start:redis": "(nohup redis-server > /dev/null 2>&1 &) && sleep 2 && tail -f /dev/null",
    "stop:redis": "pkill -f redis-server || true",
    "start:mongodb": "mongod --dbpath ./data/db",
    "start:nginx": "sudo service nginx start",
    "stop:nginx": "sudo service nginx stop",
    "edit:nginx": "sudo nano /etc/nginx/sites-available/default",
    "restart:nginx": "sudo service nginx restart",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && tsc && node dist/server.js",
    "build:frontend": "cd frontend && npm run build",
    "production": "npm run build && concurrently \"npm run serve:backend\" \"npm run serve:frontend\"",
    "serve:backend": "cd backend && node dist/server.js",
    "serve:frontend": "cd frontend && npx serve -s dist -l 3000",
    "format": "prettier --write src",
    "test": "jest",
    "lint": "eslint --ext .ts src",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:api:request-quote": "bash ./test_api/request-quote.sh",
    "clean": "cd frontend && rm -rf package-lock.json && npm install && cd ../backend && rm -rf /package-lock.json && npm install",
    "update": "cd frontend && npm update && cd ../backend &&  npm update",
    "check:env": "npx envinfo",
    "logs": "pm2 logs",
    "restart": "pm2 restart all",
    "stop": "concurrently \"npm run stop:backend\" \"npm run stop:frontend\" \"npm run stop:redis\"",
    "stop:backend": "cd backend && pkill -f 'node.*server.ts'",
    "stop:frontend": "cd frontend && pkill -f vite"
